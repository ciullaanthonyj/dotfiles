[init]
	defaultBranch = main

[core]
	pager = delta

[interactive]
	diffFilter = delta --color-only

[delta]
	line-numbers = true
	navigate = true
	light = false
	syntax-theme = Catppuccin-mocha

[merge]
	conflictstyle = diff3

[diff]
	colorMoved = default	
